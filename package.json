{
  "author": {
    "email": "luca.guzzon@gmail.com",
    "name": "Luca Guzzon",
    "url": "http://lucaguzzon.com"
  },
  "bugs": "https://github.com/lguzzon/WinUrl2Html/issues",
  "dependencies": {
    "async": "^3.2.4",
    "cli": "^1.0.1",
    "normalize-url": ">=8.0.0"
  },
  "description": "Application that replace .url windows files with an .html file with redirect to the original url",
  "devDependencies": {
    "eslint": "8.50.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "snyk": "^1.1226.0"
  },
  "homepage": "https://github.com/lguzzon/WinUrl2Html",
  "keywords": [
    "node",
    "windows"
  ],
  "license": "MIT",
  "main": "app.js",
  "name": "winurl2html",
  "repository": {
    "type": "git",
    "url": "https://github.com/lguzzon/WinUrl2Html.git"
  },
  "scripts": {
    "bld": "bun build app.js --target bun --outdir dist",
    "exe": "bun build app.js --compile --outfile winUrl2Html",
    "fix": "bunx standard --fix ./app.js && bunx eslint --fix ./app.js",
    "ins": "(npm -g list bun || npm -g i bun) && bun install",
    "test": "snyk auth && snyk test && echo \"Error: no test specified\"",
    "upd": "bunx npm-check-updates -u && bun run ins"
  },
  "version": "1.4.0"
}